<idea-plugin>
    <extensions defaultExtensionNs="com.intellij">
        <gotoDeclarationHandler
                implementation="com.github.kkkiio.intellij.protobuf.golang.PbGolangGotoDeclarationHandler"
                order="first"/>
        <definitionsScopedSearch
                implementation="com.github.kkkiio.intellij.protobuf.golang.PbGolangStructImplementationsSearch"/>
        <intentionAction>
            <bundleName>idea.plugin.protoeditor.resources.protobuf-ide</bundleName>
            <category>Go</category>
            <className>com.github.kkkiio.intellij.protobuf.golang.PbGoFillAllFieldsIntention</className>
        </intentionAction>
        <codeInsight.lineMarkerProvider language="go"
                                        implementationClass="com.github.kkkiio.intellij.protobuf.golang.MessageLineMarkerProvider"/>
    </extensions>

</idea-plugin>
